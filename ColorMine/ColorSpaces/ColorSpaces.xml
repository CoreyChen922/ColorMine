  </colorSpace>
  <colorSpace name="Hsl">
    <dataPoints>
      <dataPoint label="H"/>
      <dataPoint label="S"/>
      <dataPoint label="L"/>
    </dataPoints>
  </colorSpace>
  <colorSpace name="Lab">
    <dataPoints>
      <dataPoint label="L"/>
      <dataPoint label="A"/>
      <dataPoint label="B"/>
    </dataPoints>
  </colorSpace>
