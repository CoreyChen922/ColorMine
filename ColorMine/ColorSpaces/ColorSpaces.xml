      <dataPoint label="R" description="Red" min="0" max="255" />
      <dataPoint label="G" description="Green" min="0" max="255" />
      <dataPoint label="B" description="Blue" min="0" max="255" />
    </dataPoints>
  </colorSpace>
  <colorSpace name="Xyz">
    <dataPoints>
      <dataPoint label="X"/>
      <dataPoint label="Y"/>
      <dataPoint label="Z"/>
